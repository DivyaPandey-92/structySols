    "def max_path_sum(root):\n",
    "  if root is None:\n",
    "    return float(\"-inf\")\n",
    "  \n",
    "  if root.left is None and root.right is None:\n",
    "    return root.val\n",
    "  \n",
    "  \n",
    "  left=max_path_sum(root.left)\n",
    "  right=max_path_sum(root.right)\n",
    "    \n",
    "  return root.val+max(left, right)"
   ]
  }
